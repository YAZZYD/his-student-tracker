{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/types/types.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.vue",
     "src/preload/index.d.ts",
      "schemas/*"
  ],
  "compilerOptions": {
    "composite": true,
     "paths": {
      "@renderer/*": ["./src/renderer/src/*"],
      "@/*": ["./*"],
      "@schemas/*": ["./schemas/*"]

    }
  }
}
