{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": ["electron.vite.config.*", "src/main/**/*", "schemas/*","src/preload/**/*","ipc/**/*","services/*","prisma/generated/*", "prisma/prisma.ts"],
  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
      "paths": {
      "@/*": ["./*"],
      "@services/*": ["./services/*"],
      "@schemas/*": ["./schemas/*"]
    }
  }
}
