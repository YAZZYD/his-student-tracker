{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": ["electron.vite.config.*", "src/main/**/*", "schemas/*","src/preload/**/*","config/*","services/*","prisma/generated/*"],
  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
      "paths": {
      "@/*": ["./*"],
      "@config/*": ["./config/*"],
      "@services/*": ["./services/*"],
      "@schemas/*": ["./schemas/*"]
    }
  }
}
